{
  "name": "graphql-movie-api",
  "version": "1.0.0",
  "description": "Movie API with Graphql",
  "main": "index.js",
  "repository": "https://github.com/hanhyeonkyu/graphql-movie-api",
  "author": "AlexHan<busgod1212@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist",
    "postbuild": "cd src/graphql && copy ./schema.graphql ../../dist/graphql",
    "start": "babel-node --inspect src/index.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "graphql-yoga": "^1.14.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "copy": "^0.3.2",
    "nodemon": "^2.0.2"
  }
}
