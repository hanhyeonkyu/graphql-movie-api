    "start": "babel-node dist/index.js"
  },
  "dependencies": {
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "apollo-server-express": "^2.10.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql-tools": "^4.0.6",
    "graphql-yoga": "^1.14.8",
    "nodemon": "^2.0.2",
    "now": "^17.0.3"
  },
  "now": {
    "alias": "graphql-movie-api"
  },
  "devDependencies": {
    "copy": "^0.3.2"
  }
